
@using BlazorComponents.Shared
@using System.Linq.Expressions
@using System.Linq
@using System.Net
@using BlazorComponents.Client
@using BlazorComponents.Client.Shared
@using BlazorComponents.Client.Shared.PageDesigner
@using BlazorComponents.Shared
@page "/content/{RepositoryApiId}/List/{ListId}"
@inject HttpClient Http

@if (ListId != null && RepositoryApiId != null)
{
    <ContentList ListId=@(new Guid(ListId)) RepositoryApiId=@(new Guid(RepositoryApiId)) />
}


@functions {

    [Parameter]
    string ListId { get; set; }

    [Parameter]
    string RepositoryApiId { get; set; }

}


